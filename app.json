{
  "expo": {
    "name": "Swallow Trade",
    "slug": "swallow_trade",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/swallow_icon.png",
    "splash": {
      "image": "./assets/logo_swallowtrade_fondo_blanco.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.swallowtrade.stock"
    },
    "android": {
      "package": "com.swallowtrade.stock",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "permissions" : [ "CAMERA", "ACCESS_FINE_LOCATION" ]
    }
  }
}
