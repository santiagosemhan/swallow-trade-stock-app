{
  "expo": {
    "name": "Swallow Trade",
    "slug": "swallow_trade",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "swallowtrade",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/swallow_icon.png",
    "splash": {
      "image": "./assets/logo_swallowtrade_fondo_blanco.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.company.ios"
    },
    "android": {
      "package": "com.company.app_name",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "versionCode": 7,
      "useNextNotificationsApi": true,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}