{
  "expo": {
    "name": "Swallow Trade",
    "slug": "swallow_trade",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "swallowtrade",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/swallow_icon.png",
    "splash": {
      "image": "./assets/logo_swallowtrade_fondo_blanco.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.company.ios"
    },
    "android": {
      "package": "com.company.app_name",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "versionCode": 9,
      "useNextNotificationsApi": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "swallowtrade",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}